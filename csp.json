{
  "dev": {
    "default-src": [
      "'self' 'unsafe-inline' data:",
      "wss://localhost:3333",
      "ws://localhost:3333",
      "https://*.yourcdn.io",
      "https://*.yourcompany.com",
      "https://api.rollbar.com"
    ],
    "style-src": [
      "'self' 'unsafe-inline'",
      "https://*.google.com",
      "https://fonts.googleapis.com"
    ],
    "font-src": [
      "data:*",
      "https://fonts.gstatic.com",
      "https://fonts.googleapis.com"
    ],
    "img-src": [
      "http://localhost:*",
      "data:"
    ],
    "script-src-elem": [
      "'self' 'unsafe-inline'",
      "https://*.cloudflare.com/"
    ]
  },
  "prod": {
    "default-src": [
      "'self' 'unsafe-inline' data:",
      "https://*.yourcompany.com"
    ],
    "style-src": [
      "'self' 'unsafe-inline'",
      "https://*.yourcompany.com",
      "https://fonts.googleapis.com"
    ],
    "connect-src": [
      "'self'",
      "https://*.yourcompany.com",
      "https://*.yourcdn.io",
      "https://api.rollbar.com"
    ],
    "font-src": [
      "https://fonts.gstatic.com",
      "https://fonts.googleapis.com"
    ],
    "script-src-elem": [
      "'self' 'unsafe-inline'",
      "https://*.yourcdn.com/"
    ]
  }
}
